<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}FREE LOVE!{% endblock %}</title>
        {% block stylesheets %}
            {% stylesheets '/Resources/assets/css/*' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}

        {% endblock %}

        {% block javascripts %}
            {% javascripts
            '@AppBundle/Resources/js/jquery.js'
            '@AppBundle/Resources/js/masonry.js'
            %}
            <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>
